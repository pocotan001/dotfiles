'.workspace':
  'cmd-t': 'application:new-file'
  'cmd-n': 'application:new-window'
  'cmd-:': 'command-palette:toggle'
  'cmd-alt-:': 'project-manager:toggle'
  'cmd-alt-*': 'git-projects:toggle'
  'cmd-o': 'fuzzy-finder:toggle-file-finder'
  'cmd-shift-O': 'symbols-view:toggle-file-symbols'
  'ctrl s': 'tree-view:toggle'
  'ctrl-s': 'tree-view:toggle-focus'

'.workspace .editor:not([mini])':
  'alt-cmd-up': 'resize-panes:enlarge-active-pane'
  'alt-cmd-down': 'resize-panes:shrink-active-pane'
  'alt left': 'window:focus-pane-on-left'
  'alt right': 'window:focus-pane-on-right'
  'alt-up': 'emmet:increment-number-by-1'
  'alt-down': 'emmet:decrement-number-by-1'
  'ctrl-q': 'expand-selection-to-quotes:toggle'
  'ctrl-b': 'beautify:beautify-editor'
  'shift-enter': 'custom:semicolonize'

'atom-text-editor.autocomplete-active:not([mini])':
  'enter': 'autocomplete-plus:confirm'

'.workspace .editor[data-grammar~="html"]:not([mini])':
  'tab': 'emmet:expand-abbreviation-with-tab'
